<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>myportfolio</display-name>
	<listener>
		<listener-class>com.event.CreateInfo</listener-class>
	</listener>
	<servlet>
		<servlet-name>SendMessage</servlet-name>
		<display-name>SendMessage</display-name>
		<description></description>
		<servlet-class>com.controller.SendMessage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteMessage</servlet-name>
		<display-name>DeleteMessage</display-name>
		<description></description>
		<servlet-class>com.controller.DeleteMessage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>com.controller.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChangeCredentials</servlet-name>
		<display-name>ChangeCredentials</display-name>
		<description></description>
		<servlet-class>com.controller.ChangeCredentials</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Project</servlet-name>
		<display-name>Project</display-name>
		<description></description>
		<servlet-class>com.controller.Project</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteExperience</servlet-name>
		<display-name>DeleteExperience</display-name>
		<description></description>
		<servlet-class>com.controller.DeleteExperience</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteEducation</servlet-name>
		<display-name>DeleteEducation</display-name>
		<description></description>
		<servlet-class>com.controller.DeleteEducation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UploadResume</servlet-name>
		<display-name>UploadResume</display-name>
		<description></description>
		<servlet-class>com.controller.UploadResume</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DownloadResume</servlet-name>
		<display-name>DownloadResume</display-name>
		<description></description>
		<servlet-class>com.controller.DownloadResume</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>SendMessage</servlet-name>
		<url-pattern>/SendMessage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteMessage</servlet-name>
		<url-pattern>/DeleteMessage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChangeCredentials</servlet-name>
		<url-pattern>/ChangeCredentials</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Project</servlet-name>
		<url-pattern>/Project</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteExperience</servlet-name>
		<url-pattern>/DeleteExperience</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteEducation</servlet-name>
		<url-pattern>/DeleteEducation</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UploadResume</servlet-name>
		<url-pattern>/UploadResume</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DownloadResume</servlet-name>
		<url-pattern>/DownloadResume</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
